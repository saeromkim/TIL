//0부터 N까지의 정수 K개를 더해서 그 합이 N이 되는 경우의 수를 구하는 프로그램을 작성하시오.
//dp에서 난이도 중 문제.(어렵다. 혼자 못풀었음)

#include <iostream>
#include <algorithm>
using namespace std;
long long d[201][201];
int mod=1000000000;
int main(){
	 int n,k;
	 cin >> n >> k;
	 d[0][0] = 1LL;
	 for(int i=1; i<=k; i++){
	 	for(int j=0; j<=n; j++){
	 		for(int l=0; l<=j; l++){
	 			d[i][j]+= d[i-1][j-l];
	 			d[i][j]%=mod;
			 }
		 }
	 }

	
	
    cout << d[k][n] << '\n';
	return 0;
}
